{
  "force-preid": true,
  "nogit-commit": false,
  "nogit-tag": false,
  "git-push": true,
  "git-create-branch": false,
  "git-branch-message": "release/%s",
  "git-commit-message": "Release version: %s",
  "git-remote-name": "origin",
  "git-tag-message": "v%s",
  "increment": "patch",
  "jsonFiles": [
    {
      "file": "./gui/package.json",
      "property": "version"
    },
    {
      "file": "./gui/npm-shrinkwrap.json",
      "property": "version"
    }
  ]
}
